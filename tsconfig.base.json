{
  "compileOnSave": false,
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2018", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-ratenow/core/animations": ["libs/core/animations/src/index.ts"],
      "@nx-ratenow/core/assets": ["libs/core/assets/src/index.ts"],
      "@nx-ratenow/core/config": ["libs/core/config/src/index.ts"],
      "@nx-ratenow/core/config-panel": ["libs/core/config-panel/src/index.ts"],
      "@nx-ratenow/core/data-access-layaout": [
        "libs/core/data-access-layaout/src/index.ts"
      ],
      "@nx-ratenow/core/feature-progress-bar-loader": [
        "libs/core/feature-progress-bar-loader/src/index.ts"
      ],
      "@nx-ratenow/core/feature-user-menu": [
        "libs/core/feature-user-menu/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-footer": [
        "libs/core/layout/feature-footer/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-layaout": [
        "libs/core/layout/feature-layaout/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-navigation": [
        "libs/core/layout/feature-navigation/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-quickpanel": [
        "libs/core/layout/feature-quickpanel/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-sidenav": [
        "libs/core/layout/feature-sidenav/src/index.ts"
      ],
      "@nx-ratenow/core/layout/feature-toolbar": [
        "libs/core/layout/feature-toolbar/src/index.ts"
      ],
      "@nx-ratenow/core/material": ["libs/core/material/src/index.ts"],
      "@nx-ratenow/core/pipes": ["libs/core/pipes/src/index.ts"],
      "@nx-ratenow/core/stylesheets": ["libs/core/stylesheets/src/index.ts"],
      "@nx-ratenow/core/utils": ["libs/core/utils/src/index.ts"],
      "@nx-ratenow/shared/feature-breadcrumbs": [
        "libs/shared/feature-breadcrumbs/src/index.ts"
