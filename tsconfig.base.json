      "@nx-ratenow/shared/feature-popover": [
        "libs/shared/feature-popover/src/index.ts"
      ],
      "@nx-ratenow/shared/feature-scrollbar": [
        "libs/shared/feature-scrollbar/src/index.ts"
      ],
      "@nx-ratenow/shared/feature-secondary-toolbar": [
        "libs/shared/feature-secondary-toolbar/src/index.ts"
      ],
      "@nx-ratenow/shared/feature-sidebar": [
        "libs/shared/feature-sidebar/src/index.ts"
      ],
      "@nx-ratenow/shared/modals/feature-search-modal": [
        "libs/shared/modals/feature-search-modal/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "exclude": ["node_modules", "tmp"]
}
